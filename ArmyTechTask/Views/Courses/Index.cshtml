@model ArmyTechTask.ViewModels.CourseHomeViewModel
@{
    ViewBag.Title = "Courses";
}


<div class="row ">
    <div class="col-sm-8">
        <h3>Courses Types</h3>
        <p class="text-right"><a href="@Url.Action("CreateCourse","Courses")" class="btn btn-link"> <span class="glyphicon glyphicon-edit"></span> New Course </a></p>
        <hr />
        <div id="divListTypes">
            @Html.Partial("_ListAll", Model.Courses)
        </div>

    </div>
    <div class="col-sm-4 creation jumbotron">
        <h3>  New Course Type</h3>
        <hr />
        <div id="">
            @Html.Partial("_AddNewType", Model.NewModel)
        </div>

    </div>
</div>


@section scripts
{
    @Scripts.Render("~/bundles/jqueryval")
}

